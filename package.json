{
  "name": "react-native-launch-navigator",
  "version": "1.0.5",
  "description": "A React Native module for launching today's most popular navigation/ride apps to navigate to a destination.",
  "main": "index.js",
  "types": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpa99c/react-native-launch-navigator.git"
  },
  "homepage": "https://github.com/dpa99c/react-native-launch-navigator",
  "issue": "https://github.com/dpa99c/react-native-launch-navigator/issues",
  "author": "Dave Alden",
  "license": "MIT",
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "navigation",
    "navigator",
    "launch"
  ],
  "peerDependencies": {
    "react-native": ">=0.60.0"
  },
  "dependencies": {
    "fast-plist": "^0.1.2",
    "plist": "^2.0.1",
    "xcode": "^0.9.1"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "preuninstall": "node scripts/preuninstall.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "rnpm": {
    "commands": {
      "postlink": "node node_modules/react-native-launch-navigator/scripts/postlink.js",
      "postunlink": "node node_modules/react-native-launch-navigator/scripts/postunlink.js"
    },
    "iosQuerySchemes": [
      "citymapper",
      "comgooglemaps",
      "navigon",
      "transit",
      "waze",
      "yandexnavi",
      "uber",
      "tomtomhome",
      "com.sygic.aura",
      "here-route",
      "moovit",
      "lyft",
      "mapsme",
      "cabify",
      "baidumap",
      "taxis99",
      "iosamap"
    ]
  }
}
